<!DOCTYPE html>
<html>
    <head>
            <link rel="stylesheet" 
            href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" 
            integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO"
             crossorigin="anonymous">
    </head>
    <body>
        <% include partials/siteBanner %>
        <% include partials/navBar%>
        <p><%=character.name%></p>
        <table class='table'>
            <th>User</th>
            <th>Kills</th>
            <th>Deaths</th>
            <th>Games Played</th>
            <th>Games Won</th>
            <% for(var i = 0; i < keys.length; i++){ %>
                <tr>
                    <td><%=keys[i]%></td>
                    <td><%=character.players[keys[i]].kills%></td>
                    <td><%=character.players[keys[i]].deaths%></td>
                    <td><%=character.players[keys[i]].gamesPlayed%></td>
                    <td><%=character.players[keys[i]].gamesWon%></td>
                </tr>
            <%}%>
        </table>
    </body>
</html>